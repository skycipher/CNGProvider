    </SetEnv>

    <ItemGroup>
      <BinPlaceFilesNotExcluded Include="@(BinplaceGenerated)" Condition="'%(BinplaceGenerated.ExludedFromBuild)' != 'true'"/>
    </ItemGroup>


    <ItemGroup Condition="@(BinPlaceFilesNotExcluded) != ''">
