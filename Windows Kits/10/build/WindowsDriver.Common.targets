        TrackerLogDirectory                ="%(BinplaceGenerated.TrackerLogDirectory)"
        TLogReadFiles                      ="@(BinplaceTLogReadFiles)"
        TLogWriteFiles                     ="@(BinplaceTLogWriteFiles)"
        ToolExe                            ="$(BinplaceToolExe)"
