        ToolExe                         ="$(MessageCompileToolExe)"
        ToolPath                        ="$(MessageCompileToolPath)"
        TrackFileAccess                 ="$(TrackFileAccess)"
        MinimalRebuildFromTracking      ="%(MessageCompile.MinimalRebuildFromTracking)"
